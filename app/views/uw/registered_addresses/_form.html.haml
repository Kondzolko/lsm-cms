= simple_form_for [:uw, :coverholder, registered_address], url: uw_coverholder_registered_address_path do |f|
  = f.input :country, as: :string
  = f.input :address_line_1
  = f.input :address_line_2
  = f.input :state
  .form-group.form-row
    .col-8
      = f.input :city
    .col-4
      = f.input :zip
  = f.input :phone
  .form-group
    = f.submit 'Save', class: 'btn btn-success'